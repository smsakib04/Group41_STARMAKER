<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="groupfortyone.group41_starmaker.Salman.SearchQueryDashboardController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="215.0" layoutY="14.0" text="Search Query">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Button layoutX="499.0" layoutY="81.0" mnemonicParsing="false" onAction="#viewOA" text="View" />
            <TableView fx:id="searchTV" layoutX="57.0" layoutY="123.0" prefHeight="200.0" prefWidth="491.0">
               <columns>
                  <TableColumn fx:id="queryNoCol" prefWidth="75.0" text="Query No" />
                  <TableColumn fx:id="nameCol" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="queryCol" prefWidth="75.0" text="Query" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <Button layoutX="499.0" layoutY="345.0" mnemonicParsing="false" onAction="#previousOA" text="Previous" />
            <Button layoutX="233.0" layoutY="81.0" mnemonicParsing="false" onAction="#searchOA" text="Search" />
            <TextField fx:id="searchQueryTF" layoutX="57.0" layoutY="81.0" promptText="Search Query No." />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
